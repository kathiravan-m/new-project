{
    "name": "sparkout/post",
    "description": "",
    "keywords": [
        "sparkout",
        "post"
    ],
    "homepage": "https://github.com/sparkout/post",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "kathiravan",
            "email": "kathiravan@sparkouttech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sparkout\\Post\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparkout\\Post\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sparkout\\Post\\PostServiceProvider"
            ],
            "aliases": {
                "Post": "Sparkout\\Post\\PostFacade"
            }
        }
    }
}
